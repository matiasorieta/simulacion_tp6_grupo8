{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--ch", "100",
                "--cimax", "10",
                "--cimin", "5",
                "--tf", "600000"
            ]
        }
    ]
}